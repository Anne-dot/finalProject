' Signal Activation Routine' Used in:' 	1. M6Start.m1s' Debug flag' Const DEBUG_MODE = TrueIf DEBUG_MODE Then    Message("Preparing to activate tool signals")    Message("MuxOut: " & muxOut & " mSel: " & mSel)    Sleep(1000)End If' Activate tool signalsIf ((muxOut And 1) = 1) Then    ActivateSignal(bits0)End IfIf ((muxOut And 2) = 2) Then    ActivateSignal(bits1)End IfIf ((muxOut And 4) = 4) Then    ActivateSignal(bits2)End IfIf ((muxOut And 8) = 8) Then    ActivateSignal(bits3)End IfIf (mSel = 1) Then    ActivateSignal(m1Out)End IfIf (mSel = 2) Then    ActivateSignal(m2Out)End IfIf (mSel = 3) Then    ActivateSignal(m3Out)End IfIf DEBUG_MODE Then    Message("Tool signals activation complete")    Sleep(1000)End If