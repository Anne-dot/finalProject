' Tool Selection and Positioning Logic' Used in:'	M6Start.m1sIf DEBUG_MODE Then    Message("Selecting tool: " & tool)    Sleep(1000)End If' Tool selection and positioningSelect Case tool     Case 0        xOff = 0        yOff = 0        muxOut = 0        mSel = 0    Case 21 'M3        xOff = M3_OFFSET_X        yOff = M3_OFFSET_Y        muxOut = 0        mSel = 3    Case 22        xOff = toolSpaceX * 3        yOff = toolSpaceY * -2        muxOut = 1    Case 23        xOff = toolSpaceX * 2        yOff = toolSpaceY * -3        muxOut = 2    Case 24        xOff = toolSpaceX * 2        yOff = toolSpaceY * -2        muxOut = 3    Case 25        xOff = toolSpaceX * 2        yOff = toolSpaceY * -1	        muxOut = 4    Case 26        xOff = toolSpaceX * 2        yOff = toolSpaceY * 0	        muxOut = 5    Case 27        xOff = toolSpaceX * 2        yOff = toolSpaceY * 1	        muxOut = 6    Case 28        xOff = toolSpaceX * -2        yOff = toolSpaceY * -1	        muxOut = 7    Case 29        ' center tool        xOff = toolSpaceX * 0        yOff = toolSpaceY * 0	        muxOut = 8    Case 30        xOff = toolSpaceX * -1        yOff = toolSpaceY * 0	        muxOut = 9    Case 31        xOff = toolSpaceX * -2        yOff = toolSpaceY * 0	        muxOut = 10    Case 32        xOff = toolSpaceX * -3        yOff = toolSpaceY * 0	        muxOut = 11    Case 33        xOff = toolSpaceX * -4        yOff = toolSpaceY * 0	        muxOut = 12    Case 34        xOff = toolSpaceX * -5        yOff = toolSpaceY * 0	        muxOut = 13    Case 35        xOff = toolSpaceX * -6        yOff = toolSpaceY * 0	        muxOut = 14    Case 36        xOff = toolSpaceX * -7        yOff = toolSpaceY * 0	        muxOut = 15    Case 37        xOff = toolSpaceX * -2        yOff = toolSpaceY * -1	        muxOut = 16    Case 42        xOff = toolSpaceX * 3        yOff = toolSpaceY * -2        muxOut = 1    Case 48        xOff = toolSpaceX * -2        yOff = toolSpaceY * -1	        muxOut = 7    Case Else        muxOut = 0        If (tool >= 1 And tool <= 10) Then 'M1            xOff = M1_OFFSET_X            yOff = M1M2_OFFSET_Y            mSel = 1        Else            If (tool >= 11 And tool <= 20) Then 'M2                xOff = M2_OFFSET_X                yOff = M1M2_OFFSET_Y                mSel = 2            Else                Message "Unknown Tool"                code("M0")            End If        End IfEnd SelectIf DEBUG_MODE Then    Message("Tool positioning complete")    Message("xOff: " & xOff & " yOff: " & yOff)    Sleep(1000)End If